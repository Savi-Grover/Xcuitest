<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Functional Suite" preserve-order="true" thread-count="1" verbose="10" parallel="false" configfailurepolicy="skip"
allow-return-values="false">
	<listeners>
		<listener class-name="ios.AllureListener"/>
	</listeners>
  <test name="Signup Process" skipfailedinvocationcounts="false" preserve-order="true" group-by-instances="false" allow-return-values="false">

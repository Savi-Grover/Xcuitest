      <class name="ios.SignupFordrive"/>
    </classes>
  </test>
  <test name="Uber Signup" skipfailedinvocationcounts="false" preserve-order="true" group-by-instances="false" allow-return-values="false">
    <classes>
      <class name="ios.Signup_VehicleMarketPlace"/>
    </classes>
  </test> 
  <test name="Low Credit Signup" skipfailedinvocationcounts="false" preserve-order="true" group-by-instances="false" allow-return-values="false">
    <classes>
	  <class name="ios.LowCreditScore"/>
    </classes>
  </test> 
  <test name="Experian Fail" skipfailedinvocationcounts="false" preserve-order="true" group-by-instances="false" allow-return-values="false">
    <classes>
      <class name="ios.ExperianFail"/>
    </classes>
  </test> 
